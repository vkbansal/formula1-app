<h1>Formula 1 Championship - Season {{season}} - Races</h1>
<table class="table">
  <thead>
    <tr>
      <th>Round</th>
      <th>Name</th>
      <th>Held on</th>
    </tr>
  </thead>
  <tbody>
  {% for race in seasonData.races %}
    <tr>
      <td>{{race.round}}</td>
      <td>
        <a href="/seasons/{{season}}/round-{{race.round}}">{{race.name}}</a>
      </td>
      <td>{{race.date | humanDate}}</td>
    </tr>
  {% endfor %}
  </tbody>
</table>

