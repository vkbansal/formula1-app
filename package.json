{
  "name": "formula1-app",
  "version": "1.0.0",
  "description": "An app for Formula 1 world Championship related statistics",
  "type": "module",
  "private": true,
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx,astro}'",
    "typecheck": "astro check && tsc --noEmit",
    "dev": "astro dev",
    "start": "astro dev",
    "data:gen": "bun ./tools/generate-data.ts --allQueries",
    "data:up": "bun ./tools/update-data.ts",
    "prebuild": "rm -rf dist && rm -rf .netlify",
    "build": "NODE_ENV=production astro build",
    "fmt": "prettier --plugin-search-dir=. --write 'src/**/*.{ts,tsx,astro}'",
    "fmtc": "prettier --plugin-search-dir=. --check 'src/**/*.{ts,tsx,astro}'"
  },
  "dependencies": {
    "@astrojs/preact": "^3.1.2",
    "@astrojs/vercel": "^7.5.2",
    "astro": "^4.5.18",
    "change-case": "^5.4.4",
    "dayjs": "^1.11.10",
    "normalize.css": "^8.0.1",
    "preact": "^10.20.2",
    "sass": "^1.74.1",
    "typescript": "^5.4.4"
  },
  "devDependencies": {
    "@astrojs/check": "^0.5.10",
    "@types/cli-progress": "^3.11.5",
    "@types/node": "^20.12.7",
    "@types/papaparse": "^5.3.14",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "cheerio": "1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.34.0",
    "fast-json-stable-stringify": "^2.1.0",
    "just-omit": "^2.2.0",
    "just-safe-get": "^4.2.0",
    "meow": "^13.2.0",
    "papaparse": "^5.4.1",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "prompts": "^2.4.2",
    "typescript": "^5.1.3",
    "zx": "^7.2.3"
  },
  "pnpm": {
    "overrides": {
      "zod": "3.22.4"
    }
  }
}
