{
  "name": "formula1-app",
  "version": "1.0.0",
  "description": "An app for Formula 1 world Championship related statistics",
  "type": "module",
  "private": true,
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.{ts,tsx,astro}' 'datagen/**/*.{js,mjs}'",
    "fmt": "prettier --plugin-search-dir=. --write 'src/**/*.{ts,tsx,astro}' 'datagen/**/*.{js,mjs}'",
    "fmtc": "prettier --plugin-search-dir=. --check 'src/**/*.{ts,tsx,astro}' 'datagen/**/*.{js,mjs}'",
    "typecheck": "astro check && tsc --noEmit",
    "dev": "astro dev",
    "start": "astro dev",
    "prebuild": "rm -rf public",
    "build": "NODE_ENV=production astro build"
  },
  "dependencies": {
    "@astrojs/preact": "^1.0.2",
    "@netlify/functions": "^1.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.4",
    "astro": "1.0.5",
    "change-case": "^4.1.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.29.1",
    "globby": "^13.1.2",
    "js-yaml": "^4.1.0",
    "mongodb": "^4.8.1",
    "normalize.css": "^8.0.1",
    "preact": "^10.10.2",
    "preact-render-to-string": "^5.2.1",
    "sass": "^1.54.4",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-astro": "^0.17.1",
    "husky": "^8.0.1",
    "mariadb": "^3.0.1",
    "ora": "^6.1.2",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.5.0"
  }
}
