{
  "name": "formula1-app",
  "version": "1.0.0",
  "description": "An app for Formula 1 world Championship related statistics",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts --ext .tsx src",
    "fmt": "prettier --write 'src/**/*.{ts,tsx,scss}'",
    "clean": "rm -rf dist",
    "prestart": "yarn run clean",
    "start": "concurrently 'npm:start:*'",
    "start:css": "sass --watch src/_assets/styles/index.scss public/styles.css",
    "start:server": "npx @11ty/eleventy --serve",
    "prebuild": "yarn run clean",
    "build": "NODE_ENV=production npx @11ty/eleventy"
  },
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "change-case": "^4.1.2",
    "concurrently": "^7.2.2",
    "date-fns": "^2.28.0",
    "globby": "^13.1.2",
    "ioredis": "^5.0.6",
    "js-yaml": "^4.1.0",
    "mongodb": "^4.7.0",
    "normalize.css": "^8.0.1",
    "sass": "^1.52.3"
  },
  "devDependencies": {
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  }
}
