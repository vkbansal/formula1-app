{
  "name": "formula1-app",
  "version": "1.0.0",
  "description": "An app for Formula 1 world Championship related statistics",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}' 'scripts/**/*.{js,mjs}'",
    "fmt": "prettier --write 'src/**/*.{ts,tsx}' 'scripts/**/*.{js,mjs}'",
    "fmt:check": "prettier --check 'src/**/*.{ts,tsx}' 'scripts/**/*.{js,mjs}'",
    "prestart": "rm -rf public",
    "start": "npx @11ty/eleventy --serve --formats=tsx --quiet",
    "data": "wget http://ergast.com/downloads/f1db.sql.gz -O initdb.d/f1db.sql.gz",
    "prebuild": "rm -rf public",
    "build": "NODE_ENV=production node scripts/esbuild.mjs && npx @11ty/eleventy --formats=tsx",
    "datagen": "node ./scripts/datagen/index.mjs generate"
  },
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-canary.12",
    "@emotion/css": "^11.9.0",
    "@types/d3": "^7.4.0",
    "builtin-modules": "^3.3.0",
    "change-case": "^4.1.2",
    "classnames": "^2.3.1",
    "cssnano": "^5.1.12",
    "cssnano-preset-default": "^5.2.12",
    "date-fns": "^2.28.0",
    "esbuild": "^0.14.48",
    "eval": "^0.1.8",
    "fast-json-stable-stringify": "^2.1.0",
    "globby": "^13.1.2",
    "happy-dom": "^6.0.3",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.14",
    "preact": "^10.9.0",
    "preact-render-to-string": "^5.2.0",
    "specificity": "^0.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chalk": "^5.0.1",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "js-yaml": "^4.1.0",
    "mariadb": "^3.0.0",
    "ora": "^6.1.2",
    "path-to-regexp": "^6.2.1",
    "prettier": "^2.7.1",
    "slugify": "^1.6.5",
    "typescript": "^4.7.4",
    "yargs": "^17.5.1"
  }
}
