{
  "name": "formula1-app",
  "version": "1.0.0",
  "description": "An app for Formula 1 world Championship related statistics",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .ts --ext .tsx src",
    "fmt": "prettier --write 'src/**/*.{ts,tsx,scss}'",
    "prestart": "rm -rf public",
    "start": "concurrently npm:start:*",
    "start:assets": "node scripts/esbuild.mjs --watch",
    "start:11ty": "npx @11ty/eleventy --serve --incremental",
    "build": "node scripts/build.mjs",
    "datagen": "node ./scripts/datagen/index.mjs generate"
  },
  "author": "Vivek Kumar Bansal <contact@vkbansal.me>",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.1",
    "change-case": "^4.1.2",
    "date-fns": "^2.28.0",
    "esbuild": "^0.14.48",
    "globby": "^13.1.2",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-modules": "^4.3.1",
    "postcss-nested": "^5.0.6",
    "zx": "^7.0.7"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "chalk": "^5.0.1",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "js-yaml": "^4.1.0",
    "mariadb": "^3.0.0",
    "ora": "^6.1.2",
    "path-to-regexp": "^6.2.1",
    "prettier": "^2.7.1",
    "slugify": "^1.6.5",
    "yargs": "^17.5.1"
  }
}
