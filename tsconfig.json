{
	"compilerOptions": {
		"baseUrl": "src",
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"moduleResolution": "Node",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"types": ["astro/client"],
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment"
	},
	"include": ["src"]
}
